<h1>Editing <%= @subtask.title %></h1>

<%= form_for @subtask  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <p>Assign at least One Household Member to Subtask</p>
    <%= f.collection_check_boxes :user_ids, User.all, :id, :username %>
    <br />

  <%= f.submit %>
<% end %>
