<h1>Your assigned subtasks:</h1>
<ul>
<% @user.subtasks.each do |subtask| %>
  <li><%= @add_link = '<a href="' + "../tasks/#{subtask.task_id}/subtasks/#{subtask.id}" + '">' + "#{subtask.title}" + '</a>' %>
  - <% if subtask.complete == true %>
     Complete
  <% else %>
    Incomplete
  <% end %>
  </li>
<% end %>
</ul>

<br>

<form action="../tasks/new"><button type="submit">Create New Task</button></form><br><br>


